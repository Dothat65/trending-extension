{
    "manifest_version": 3,
    "name": "Restaurant Recs",
    "description": "Find pros and cons and youtube shorts for restaurants",
    "version": "1.0",
    "action": {
        "default_popup" : "index.html"
    },
    "icons": {
    },
    "content_scripts": [
        {
            "js": ["./content.js"],
            "matches": ["https://www.google.com/maps*"]
        }
    ],
    "permissions": [
        "storage"
    ]
}
